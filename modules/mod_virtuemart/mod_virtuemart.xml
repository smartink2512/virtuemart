		<param name="class_sfx" type="text" default=""
			label="MOD_VIRTUEMART_MENU_CLASS_SUFFIX"
			description="MOD_VIRTUEMART_MENU_CLASS_SUFFIX_DESC" />
		<param name="moduleclass_sfx" type="text" default=""
			label="MOD_VIRTUEMART_MOD_CLASS_SUFFIX"
			description="MOD_VIRTUEMART_MOD_CLASS_SUFFIX_DESC" />
		<param name="pretext" type="text" default="" label="MOD_VIRTUEMART_PRE_TEXT"
			description="MOD_VIRTUEMART_PRE_TEXT_DESC" />
		<param name="show_login_form" type="radio" default="no"
			label="MOD_VIRTUEMART_LOGIN_FORM"
			description="MOD_VIRTUEMART_LOGIN_FORM_DESC">
			<option value="yes">Yes</option>
			<option value="no">No</option>
		</param>
		<param name="show_categories" type="radio" default="yes"
			label="MOD_VIRTUEMART_SHOW_PRODUCT_CATEGORIES"
			description="MOD_VIRTUEMART_SHOW_PRODUCT_CATEGORIES_DESC">
			<option value="yes">Yes</option>
			<option value="no">No</option>
		</param>
		<param name="show_listall" type="radio" default="yes"
			label="MOD_VIRTUEMART_LIST_ALL"
			description="MOD_VIRTUEMART_LIST_ALL=_DESC">
			<option value="yes">Yes</option>
			<option value="no">No</option>
		</param>
		<param name="show_adminlink" type="radio" default="yes"
			label="MOD_VIRTUEMART_ADMIN_LINK"
			description="MOD_VIRTUEMART_ADMIN_LINK_DESC">
			<option value="yes">Yes</option>
			<option value="no">No</option>
		</param>
		<param name="show_accountlink" type="radio" default="yes"
			label="MOD_VIRTUEMART_ACCOUNT_LINK"
			description="MOD_VIRTUEMART_ACCOUNT_LINK_DESC">
			<option value="yes">Yes</option>
			<option value="no">No</option>
		</param>
		<param name="useGreyBox_accountlink" type="radio" default="0"
			label="MOD_VIRTUEMART_ACCOUNT_GREYBOX"
			description="MOD_VIRTUEMART_ACCOUNT_GREYBOX_DESC">
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="show_minicart" type="radio" default="yes"
			label="MOD_VIRTUEMART_MINICART"
			description="MOD_VIRTUEMART_MINICART_DESC">
			<option value="yes">Yes</option>
			<option value="no">No</option>
		</param>
		<param name="useGreyBox_cartlink" type="radio" default="0"
			label="MOD_VIRTUEMART_MINICART_GREYBOX"
			description="MOD_VIRTUEMART_MINICART_GREYBOX_DESC">
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="show_productsearch" type="radio" default="yes"
			label="MOD_VIRTUEMART_SEARCH"
			description="MOD_VIRTUEMART_SEARCH_DESC">
			<option value="yes">Yes</option>
			<option value="no">No</option>
		</param>
		<param name="show_product_parameter_search" type="radio"
			default="no" label="Show Product Parameter Search Field?"
			description="Shows a link to the Product Parameter Search Form (advanced search!).">
			<option value="yes">Yes</option>
			<option value="no">No</option>
		</param>
		<param name="menutype" type="list" default="links"
			label="MOD_VIRTUEMART_CATEGORY_DISPLAY"
			description="MOD_VIRTUEMART_CATEGORY_DISPLAY_DESC">
